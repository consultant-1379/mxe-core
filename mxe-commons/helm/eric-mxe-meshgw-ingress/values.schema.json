{
    "$schema": "http://json-schema.org/schema#",
    "type": "object",
    "properties": {
        "annotations": {
            "type": "object"
        },
        "enabled": {
            "type": "boolean"
        },
        "ingress": {
            "type": "object",
            "properties": {
                "annotationPrefix": {
                    "oneOf": [
                        { "type": "string"},
                        { "type": "null" }
                      ]
                },
                "class": {
                    "oneOf": [
                        { "type": "string"},
                        { "type": "null" }
                      ]
                },
                "defaultBackendFullNameOverride": {
                    "oneOf": [
                        { "type": "string"},
                        { "type": "null" }
                      ]
                },
                "defaultBackendNameOverride": {
                    "oneOf": [
                        { "type": "string"},
                        { "type": "null" }
                      ]
                },
                "enabled": {
                    "type": "boolean"
                },
                "hostname": {
                    "oneOf": [
                        { "type": "string"},
                        { "type": "null" }
                      ]
                },
                "ingressClass": {
                    "type": "string"
                },
                "modelServiceOwasp": {
                    "type": "object",
                    "properties": {
                        "enabled": {
                            "type": "boolean"
                        }
                    }
                },
                "owasp": {
                    "type": "object",
                    "properties": {
                        "enabled": {
                            "type": "boolean"
                        }
                    }
                },
                "secretName": {
                    "oneOf": [
                        { "type": "string"},
                        { "type": "null" }
                      ]
                }
            }
        },
        "labels": {
            "type": "object"
        },
        "nameOverride": {
            "oneOf": [
                { "type": "string"},
                { "type": "null" }
              ]
        },
        "productInfo": {
            "type": "object",
            "properties": {
                "rstate": {
                    "type": "string"
                }
            }
        },
        "service": {
            "type": "object",
            "properties": {
                "fullnameOverride": {
                    "type": "string"
                },
                "nameOverride": {
                    "oneOf": [
                        { "type": "string"},
                        { "type": "null" }
                      ]
                }
            }
        },
        "terminationGracePeriodSeconds": {
            "type": "integer"
        },
        "tolerations": {
            "type": "array"
        }
    }
}
